stingeripc:
  version: "0.0.2"

interface:
  name: ParamOnly
  version: 0.0.1

properties:
  doubleParam:
    payload:
      args:
        one:
          type: integer
        two:
          type: string
      initialValue:
        one: 1
        two: "two"
  
  singleParam:
    payload:
      args:
        foo:
          type: string
      initialValue:
        foo: "hello world"